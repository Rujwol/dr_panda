<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent">


    <Button
        android:id="@+id/create_p_but"
        android:layout_width="150dp"
        android:layout_height="100dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_marginLeft="@dimen/tile_margin"
        android:layout_marginTop="@dimen/tile_margin"
        android:background="@color/colorAccent"
        android:text="@string/create_prof_string" />


    <Button
        android:id="@+id/view_p_but"
        android:layout_width="150dp"
        android:layout_height="100dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentRight="true"
        android:layout_marginRight="@dimen/tile_margin"
        android:layout_marginTop="@dimen/tile_margin"
        android:background="@color/colorAccent"
        android:text="@string/view_prof_string" />
